<% include ../layout/header.ejs %>
<!-- END TOPBAR -->
<!-- START CONTAINER -->
<div class="page-container row-fluid container-fluid">
    <!-- SIDEBAR - START -->
   
         <% include ../layout/menu.ejs %>

    <!--  SIDEBAR - END -->
    <!-- START CONTENT -->
    <section id="main-content" class=" ">
        <section class="wrapper main-wrapper row">
            <div class='col-xs-12'>
                <div class="page-title">
                    <div class="pull-left">
                        <h1 class="title">Danh Sách File</h1>
                    </div>
                    <div class="pull-right hidden-xs">
                       
                    </div>
                </div>
            </div>
            <div class="clearfix"></div>
            <div class="col-lg-12">
                <section class="box ">
                    <header class="panel_header">
                        <h2 class="title pull-left">Tất cả các tệp</h2>
                    </header>
                    <div class="content-body">
                        <div class="row">
                            <% if(success_msg != ''){ %> 
                                <div class="alert alert-success"><%= success_msg%></div>
                           <% } %>
                            <% if(error_msg != ''){ %> 
                                <div class="alert alert-success"><%= error_msg%></div>
                           <% } %>
                            <div class="col-xs-12">
                                <!-- ********************************************** -->
                                <table id="table" class="display table table-hover table-condensed">
                                    <thead>
                                        <tr>
                                            <th>Index</th>
                                            <th>Account Upload</th>
                                            <th>File Upload</th>
                                            <th>File Convert</th>
                                            <th>Control</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% var id = 0 %>
                                        <% listFile.forEach(function(one){ id ++%>
                                        <tr>
                                            <td><%= id%></td>
                                            <td><%= one.username%></td>
                                            <td><a href="/admin/file/<%= one._id %>/tai-file-upload"><%= one.file_upload%></a></td>
                                            <td><a href="/admin/file/<%= one._id %>/tai-file-convert"><%= one.file_converted%></a></td>
                                            <td>
                                                <a href="/admin/file/<%= one._id %>/xoa-file">Xoá</a>
                                            </td>
                                        </tr>
                                        <%})%>
                                    </tbody>
                                </table>
                                <!-- ********************************************** -->
                            </div>
                        </div>
                    </div>
                </section>
            </div>
        </section>
    </section>
    <!-- END CONTENT -->
    <div class="chatapi-windows ">
    </div>

<% include ../layout/footer.ejs %>
<% include ../library.ejs %> 